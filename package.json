{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build",
    "start": "node ace serve --watch",
    "lint": "eslint . --ext=.ts",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^2.1.3",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "adonis-preset-ts": "^1.0.4",
    "eslint": "^7.3.1",
    "eslint-plugin-adonis": "^1.0.14",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "pino-pretty": "^4.0.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6",
    "youch": "^2.0.10",
    "youch-terminal": "^1.0.0"
  },
  "dependencies": {
    "@adonisjs/ace": "^6.9.3",
    "@adonisjs/core": "^5.0.0-preview-rc-1.9",
    "@adonisjs/fold": "^6.3.5",
    "@adonisjs/lucid": "^8.2.2",
    "git-commit-msg-linter": "^2.4.4",
    "pg": "^8.2.1",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  }
}
